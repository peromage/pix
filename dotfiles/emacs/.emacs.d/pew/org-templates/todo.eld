;;; -*- coding: utf-8; mode: lisp-data; -*-

(;; General
 (sequence "TODO(t)" "IN-PROGRESS(p)"
           "|"
           "DONE(d!/!)" "ABORT(c@/@)")

 ;; Jira style
 (sequence "ANALYSIS(!/!)" "DEVELOPING(!/!)" "INTEGRATING(!/!)" "VERIFYING(!/!)"
           "|"
           "ACCEPTED(!/!)" "CANCELLED(@/@)"))
